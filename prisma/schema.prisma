generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  name  String? @db.VarChar(255)
  email String  @unique @db.VarChar(255)
  keys  APIKey?
}

model APIKey {
  id     Int     @id @default(autoincrement())
  key    String?
  userId Int     @unique
  User   User    @relation(fields: [userId], references: [id])
}
